{
    "manifest_version": 2,
    "name": "CSX Solutions",
    "description": "Shows solutions to CSX problems",
    "version": "1.0.0",
    "icons": {
      "128": "images/csxsolutions2.JPG"
    },
    "page_action": {
      "default_icon": {
        "128": "images/csxsolutions2.JPG"
        }
      },
    "permissions": [
        "activeTab",
        "tabs"
        ],
    "web_accessible_resources": ["index.html", "solutions.js", "style.css"],

    "content_scripts": [{
      "matches": ["https://csx.codesmith.io/units/*"],
      "js": ["hard-learning.js"]
      }]
    
}
